{
  "name": "frappe-ui-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn builddashboard && yarn buildpublic",
    "builddashboard": "vite build --base=/assets/erpnext_kleingartenverein/frontend/dashboard && yarn copy-html-entry && yarn create-script-template && yarn create-js-template",
    "buildpublic": "vite build -c vite.public.config.js --base=/assets/erpnext_kleingartenverein/frontend/pub && yarn copy-service-entry && yarn create-public-script-template && yarn create-public-js-template",
    "preview": "vite preview",
    "copy-html-entry": "cp ../erpnext_kleingartenverein/public/frontend/dashboard/index.html ../erpnext_kleingartenverein/www/dashboard.html",
    "copy-service-entry": "cp ../erpnext_kleingartenverein/public/frontend/pub/service.html ../erpnext_kleingartenverein/www/service_template.html",
    "create-script-template": "cat ../erpnext_kleingartenverein/www/dashboard.html | grep -E  \"stylesheet\" > ../erpnext_kleingartenverein/templates/script.html ",
    "create-js-template": "cat ../erpnext_kleingartenverein/www/dashboard.html | grep -E  \"module\" > ../erpnext_kleingartenverein/templates/js.html ",
    "create-public-script-template": "cat ../erpnext_kleingartenverein/www/service_template.html | grep -E  \"stylesheet\" > ../erpnext_kleingartenverein/templates/public_script.html ",
    "create-public-js-template": "cat ../erpnext_kleingartenverein/www/service_template.html | grep -E  \"module\" > ../erpnext_kleingartenverein/templates/public_js.html "
  },
  "dependencies": {
    "@vueuse/core": "^10.2.1",
    "feather-icons": "^4.29.0",
    "flowbite": "^1.8.1",
    "frappe-ui": "^0.0.112",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-cal": "^4.8.1",
    "vue-i18n": "9",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vite": "^2.7.2"
  }
}
