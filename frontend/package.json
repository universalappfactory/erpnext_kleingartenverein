{
  "name": "frappe-ui-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/assets/erpnext_kleingartenverein/frontend/ && yarn copy-html-entry && yarn create-script-template && yarn create-js-template",
    "preview": "vite preview",
    "copy-html-entry": "cp ../erpnext_kleingartenverein/public/frontend/index.html ../erpnext_kleingartenverein/www/dashboard.html",
    "create-script-template": "cat ../erpnext_kleingartenverein/www/dashboard.html | grep -E  \"stylesheet\" > ../erpnext_kleingartenverein/templates/script.html ",
    "create-js-template": "cat ../erpnext_kleingartenverein/www/dashboard.html | grep -E  \"module\" > ../erpnext_kleingartenverein/templates/js.html "
  },
  "dependencies": {
    "feather-icons": "^4.28.0",
    "flowbite": "^1.6.6",
    "frappe-ui": "^0.0.112",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.15",
    "vite": "^2.7.2"
  }
}
